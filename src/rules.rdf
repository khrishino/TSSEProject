    ?spThing1 tracking:hasBoundingBox ?bb1 .
    ?spThing2 tracking:hasBoundingBox ?bb2 .
    ?bb1 tracking:bottomLeftVertex ?point1 .
    ?bb2 tracking:bottomLeftVertex ?point2 .
