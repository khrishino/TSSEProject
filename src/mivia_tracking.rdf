<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:tracking="http://mivia.unisa.it/videotracking/tracking.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.w3.org/2002/07/owl">
  <owl:Ontology rdf:about="">
    <rdfs:isDefinedBy>Pierluigi Ritrovato and Claudia Grieco</rdfs:isDefinedBy>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >owl:Ontology added by TopBraid</owl:versionInfo>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"
    >1.1</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Squared">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OccludingObject"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >occluding object that is in square form</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass>
          <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#BoundingBox"/>
        </owl:onClass>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasBoundingBox"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#BackgroundObject">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/dc/terms/Agent">
    <owl:equivalentClass>
      <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdfs:comment>An agent (eg. person, group, software or physical artifact).</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#TrackedObject"/>
    </rdfs:subClassOf>
    <rdfs:label>Agent</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Color"/>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Frame">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Stationary">
    <rdfs:comment xml:lang="en">a tracked object stay stationary.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#LeavingAGroup">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#GhostObject">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#SceneObject"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#JoinAGroup">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#EntryArea">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Entry Area</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#ThinVertical">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OccludingObject"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >occluding object that causes vertical splitting</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#StaticObject">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#BackgroundObject"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Area">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing"/>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
    <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <rdfs:comment>A person.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>Person</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Point">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#negativeInteger"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#y"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#negativeInteger"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#x"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Object">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#BoundingBox">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bounding Box of any Spatial Thing</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#topLeftVertex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#bottomRightVertex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#bottomLeftVertex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Point"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#topRightVertex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Position"/>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Blob">
    <rdfs:comment xml:lang="en">A joint set of pixels detected by the low-level algorithm</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Appeareance"/>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#HSVColor">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Color"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#SceneObject">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Object"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#TrackedObject">
    <rdfs:comment xml:lang="en">A tracked object corresponds to an entity identified from blobs detected by the lowlevel algorithm.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SceneObject"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#ThinHorizontal">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OccludingObject"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >occluding object that causes horizontal splitting</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Scattered">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OccludingObject"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >occluding object that causes scattering</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Noise">
    <rdfs:comment xml:lang="en">Noise object to be ignored.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SceneObject"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#ExitArea">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
    <rdfs:label xml:lang="en">Exit Area</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2000/10/swap/pim/contact#Person">
    <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OArea">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
    <rdfs:label xml:lang="en">Occlusion Area</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#SpatialPerspectiveArea">
    <rdfs:comment xml:lang="en">Area in which a specified zoom factor is applied</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
    <rdfs:label xml:lang="en">Spatial Perspective Area</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Person">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#OccludingObject">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#BackgroundObject"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#YUVColor">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Color"/>
  </owl:Class>
  <owl:Class rdf:about="http://schema.org/Person">
    <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#RGBColor">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Color"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#MovingObject">
    <rdfs:subClassOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#BackgroundObject"/>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Sequence">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Frame"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isComposedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass>
          <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Camera"/>
        </owl:onClass>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isRecordedWith"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#EnteringScene">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#LeavingScene">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Group">
    <rdfs:comment>A class of Agents.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label>Group</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#Situation">
    <rdfs:comment xml:lang="en">represent different situations where a tracked object may be.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isRawDataOf">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasRawData"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Frame"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#bottomRightVertex">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasVertex"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasVertex">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Vertex of a bounding Box. vertex are Point</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#firstSeenAt">
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Frame"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#TrackedObject"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >frame in which a tracked object is first seen</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasMovingInformation">
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#TrackedObject"/>
    <rdfs:comment>it should be removed</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#belongTo">
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Frame"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasCenter">
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Point"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#BoundingBox"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >center of bounding box</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasPosition">
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing"/>
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Position"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#hasBoundingBox">
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#SpatialThing"/>
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#BoundingBox"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#topRightVertex">
    <rdfs:subPropertyOf rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#hasVertex"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#interferWith">
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isLocatedin">
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Object"/>
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isMergedWith">
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >indicates the blobs that are merged according to some reasons</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#contains">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://mivia.unisa.it/videotracking/tracking.owl#isContainedIn"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Blob"/>
    <rdfs:domain rdf:resource="http://mivia.unisa.it/videotracking/tracking.owl#Area"/>
